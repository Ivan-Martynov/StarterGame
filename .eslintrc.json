{
	"env": {
		"commonjs": true,
		"es2021": true,
		"browser": true,
		"jest": true
	},
	"extends": [
		"eslint:recommended",
		"prettier"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 12
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
		"no-unused-vars": "warn"
	}
}
